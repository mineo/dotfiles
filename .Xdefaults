*urgentOnBell: true
*scrollBar: false
*saveLines: 10000
*font: xft:Consolas:pixelsize=14,xft:Symbola:pixelsize=12
*internalBorder: 0
*colorUL: #c5f779
*underlineColor: #c5f779

!urxvt*background: #1a1a1a
!urxvt*foreground: #ccc
urxvt*transparent: true
urxvt*shading: 15
urxvt*fading: 50

!!url stuff
URxvt.perl-ext:      default,url-select
URxvt.keysym.M-u:    perl:url-select:select_next
URxvt.url-select.launcher:   /usr/bin/firefox -new-tab
URxvt.url-select.underline: true

Xcursor.size: 16

#include ".colors/mine"
