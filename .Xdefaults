*urgentOnBell: true
*scrollBar: false
*saveLines: 10000
*font: xft:Consolas:pixelsize=16,xft:Symbola:pixelsize=12
*internalBorder: 0
*colorUL: #c5f779
* underlineColor: #c5f779

urxvt*background: #1a1a1a
urxvt*foreground: #ccc
!urxvt*transparent: true
!urxvt*shading: 35
!urxvt*fading: 50

!url stuff
*perl-ext-common:	default,matcher,url-select
*urlLauncher: firefox
*matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
*matcher.button: 1
*keysym.M-u: perl:url-select:select_next
*underlineURLs: true

